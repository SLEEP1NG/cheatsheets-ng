{
  "name": "cheatsheets",
  "version": "1.0.0",
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "prettier-eslint-cli": "^4.7.1"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "echo \"Try 'make build' or 'make build-prefixed'\"; exit 1",
    "deploy": "echo \"Try 'make build' (or 'build-prefixed') then 'make deploy'\"; exit 1",
    "develop": "make develop",
    "lint:check": "./support/check.sh",
    "lint:fix": "./support/format.sh",
    "prettier:check": "prettier-eslint --list-different 'sheets/**/*.md'",
    "prettier:fix": "prettier-eslint --write 'sheets/**/*.md'",
    "status": "./support/status.sh"
  }
}
